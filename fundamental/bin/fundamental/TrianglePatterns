package fundamental;

import java.util.Scanner;
public class TrianglePatterns{
    Scanner sc = new Scanner(System.in);
    int n=sc.nextInt();

    int i=1;
    while(i<=n){
        int j=1;
        while(j<=n){
            System.out.print(i);
        }
        System.out.print("\n");
        j++;
        i++;

    }
}